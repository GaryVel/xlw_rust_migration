<?xml version="1.0" encoding="utf-8"?>

<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003" ToolsVersion="16.0">
  <Import Project="$(MSBuildThisFileDirectory)xlw.version.props"/>
  <PropertyGroup>
    <xlwPkgBuildDir>$(MSBuildThisFileDirectory)..</xlwPkgBuildDir>
  </PropertyGroup>

  <PropertyGroup>
    <XLWROOT>$(MSBuildThisFileDirectory)..</XLWROOT>
    <XLWDOTNETDIR>$(XLWROOT)\mixed</XLWDOTNETDIR>
    <XLWBIN>$(XLWROOT)\native\lib</XLWBIN>
  </PropertyGroup>
  
  <ItemGroup>
    <Compile Remove="_xlwAutoGeneratedSource\**" />
    <EmbeddedResource Remove="_xlwAutoGeneratedSource\**" />
    <None Remove="_xlwAutoGenerated\**" />
  </ItemGroup>


  <ItemGroup>
    <xlwcppprojectFiles Include="$(XLWROOT)\_xlwAutoGenerated\_xlwAutoGeneratedSource\*.*"/>
  </ItemGroup>


</Project>