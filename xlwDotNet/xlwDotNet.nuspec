<?xml version="1.0"?>
<package >
    <metadata>
        <id>xlwDotNet</id>
        <version>$version$</version>
        <description>xlw - Excel xlls made easy</description>
        <tags>.NET Core Managed Excel xll C API</tags>
        <authors>https://github.com/xlw</authors>
        <license type="file">LICENSE.TXT</license>
        <repository type="git" url="https://github.com/xlw/xlw.git" />
        <requireLicenseAcceptance>true</requireLicenseAcceptance>
        <icon>images\logo.jpg</icon>
    </metadata>
     
    <files>
        <file src="..\xlw\lib\Win32\*.*"      target="native\lib\Win32\"     />
        <file src="..\xlw\lib\x64\*.*"        target="native\lib\x64\"     />
        <file src="..\xlw\lib\*.exe"          target="native\lib\"     />
        <file src="..\xlw\include\xlw\*.*"    target="native\include\xlw\" />

        <file src="include\xlwDotNet\*.*"     target="mixed\include\xlwDotNet\" />
        <file src="lib\**"                    target="mixed\lib\" />
      
      
      
        <file src="nuget_content\**"          target="_xlwAutoGenerated" />
        <file src="xlwDotNet.props"           target="build\" />
        <file src="xlwDotNet.targets"         target="build\" />
        <file src="..\xlw.version.props"      target="build\" />
        <file src="..\logo.jpg"               target="images\" />
        <file src="..\LICENSE.TXT"            target="" />
    </files>

 
</package>