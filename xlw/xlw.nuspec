<?xml version="1.0"?>
<package >
    <metadata>
        <id>xlw</id>
        <version>6.0.0</version>
        <description>XLW</description>
        <tags>Native, native</tags>
        <authors>XLW</authors>

      <contentFiles>
        <files include="*.cpp" buildAction="Compile" />
        <files include="*.h"   buildAction="None" />
      </contentFiles>
    </metadata>
     
    <files>
        <file src="lib\Win32\*.*"    target="build\native\lib\Win32\"     />
        <file src="lib\x64\*.*"      target="build\native\lib\x64\"     />
        <file src="lib\*.exe"        target="build\native\lib\"     />
        <file src="include\xlw\*.*"  target="build\native\include\xlw\" />
        <file src="xlw.props"        target="build\" />
        <file src="xlw.Cpp.props"    target="build\native" />
        <file src="xlw.Cpp.targets"  target="build\native" />
        <file src="nuget_content\**" target="content" />
    </files>

 
</package>