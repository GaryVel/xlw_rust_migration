<?xml version="1.0"?>
<package >
    <metadata>
        <id>xlw</id>
        <version>$version$</version>
        <description>xlw - Excel xlls made easy</description>
        <tags>Native Excel xll C API</tags>
        <authors>https://github.com/xlw</authors>
        <license type="file">LICENSE.TXT</license>
        <repository type="git" url="https://github.com/xlw/xlw.git" />
        <requireLicenseAcceptance>true</requireLicenseAcceptance>
        <icon>images\logo.jpg</icon>
      <contentFiles>
        <files include="*.cpp"     buildAction="Compile" />
        <files include="*.h"       buildAction="None" />
        <files include="*.props"   buildAction="None" />
      </contentFiles>
    </metadata>
     
    <files>
        <file src="lib\Win32\*.*"      target="native\lib\Win32\"     />
        <file src="lib\x64\*.*"        target="native\lib\x64\"     />
        <file src="lib\*.exe"          target="native\lib\"     />
        <file src="include\xlw\*.*"    target="native\include\xlw\" />
        <file src="xlw.props"          target="build\" />
        <file src="xlw.Cpp.props"      target="build\" />
        <file src="xlw.Cpp.targets"    target="build\" />
        <file src="..\xlw.version.props"  target="build\" />
        <file src="nuget_content\**"   target="content" />
        <file src="..\logo.jpg"        target="images\" />
        <file src="..\LICENSE.TXT"     target="" />
    </files>

 
</package>